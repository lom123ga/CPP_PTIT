#include <bits/stdc++.h>

using namespace std;

#define endl "\n"
#define faster() \
    ios_base::sync_with_stdio(0); \
    cin.tie(NULL); \
    cout.tie(NULL);
#define fi first
#define se second
#define INF 1e9
#define all(x) x.begin(),x.end()
#define pii pair<int,int>
typedef long long ll;
typedef double dbl;

const ll M = 1e9+7;

vector<bool> isPrime(1000005,true);
void sieve(){
    isPrime[0] = isPrime[1] = false;
    for (int i=2;i*i<1000005;i++){
        if (isPrime[i]){
            for (int j=i*i;j<1000005;j+=i) isPrime[j] = false;
        }
    }
}


void solve(){
    int k;
    string s;
    cin >> s;
    cin >> k;
    int a[26] = {0};
    for (int i=0;i<s.length();i++){
        a[s[i]-'a']++;
    }
    for (int i=0;i<26;i++)
        if (a[i] == 0) k--;
    if (k >=0) cout << 1 << endl;
    else cout << 0 << endl;
}

int main(){
    faster();
    //sieve();
    int t;
    t = 1;
    cin >> t;
    //cin.ignore();
    while (t--){
        solve();
    }
    return 0;
}
